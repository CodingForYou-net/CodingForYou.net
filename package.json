{
  "name": "codingforyou.net",
  "description": "codingforyou.net - website",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run start:db && sapper dev --live false",
    "start": " npm run start:db && node __sapper__/build",
    "start:whf": "stripe listen --events payment_intent.payment_failed,payment_intent.succeeded --forward-to localhost:3000/api/stripe/webhook",
    "start:db": "if ! (pgrep mongod); then mongod --config /usr/local/etc/mongod.conf --fork; fi",
    "stop:db": "pkill mongod || true",
    "build": "npm run build:sapper && run-p --race start build:sitemap",
    "build:sapper": "sapper build --legacy",
    "build:sitemap": "sleep 10 && sitemap-generator -f ./static/sitemap.xml http://localhost:3000 && replace-in-files --string 'http://localhost:3000' --replacement 'https://codingforyou.net' ./static/sitemap.xml",
    "export": "sapper export --legacy",
    "predeploy": "npm i && npm run build",
    "deploy": "pm2 stop codingforyou.net && pm2 start pm2.config.js && pm2 save"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "clear": "^0.1.0",
    "colors": "^1.4.0",
    "compression": "^1.7.1",
    "es6-promise-polyfill": "^1.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "format-message": "^6.2.3",
    "is-mobile": "^2.2.1",
    "memorystore": "^1.6.2",
    "mongoose": "^5.9.4",
    "nodemailer": "^6.4.5",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "setimmediate": "^1.0.5",
    "sirv": "^0.4.0",
    "stripe": "^8.32.0",
    "svelte-intl": "^1.1.0",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "import-sort-style-module": "^6.0.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-plugin-import-sort": "0.0.3",
    "replace-in-files-cli": "^0.3.1",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.10",
    "sitemap-generator-cli": "^7.5.0",
    "svelte": "^3.20.0",
    "svelte-preprocess": "^3.5.0"
  },
  "importSort": {
    ".js": {
      "style": "module"
    }
  }
}
